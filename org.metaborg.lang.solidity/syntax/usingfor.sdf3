module usingfor

imports
  Common
  types

template options
  tokenize : "(;"

context-free start-symbols
  UsingFor

lexical syntax
  KEYWORD = USINGKEYWORD
  USINGKEYWORD = "using"
  USINGKEYWORD = "for"

context-free syntax
  UsingFor.UsingFor = <using <PATH> for <UsingForItem>;>
  
  UsingForItem.ForStar = <*>
  UsingForItem.ForType = <<ElementaryType>>
  
